<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
            android:fromDegrees="-135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="-135">
    <item android:id="@android:id/background">
        <shape
            android:innerRadiusRatio="2.1"
            android:shape="ring"
            android:useLevel="false"
            android:angle="0"
            android:type="sweep"
            android:thicknessRatio="40.0">
            <solid android:color="@color/white_inactive"/>
        </shape>

    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="-135"
            android:toDegrees="-135">
            <shape
                android:innerRadiusRatio="2.1"
                android:shape="ring"
                android:angle="0"
                android:type="sweep"
                android:thicknessRatio="40.0">
                <solid android:color="@color/white_active"/>
            </shape>
        </rotate>
    </item>
</layer-list>
